<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="AppiumMobileSuite"
     skipfailedinvocationcounts="false" junit="false" parallel="false"
     data-provider-thread-count="50" annotations="JDK">
  <!-- SELENIUM CONFIGURATION -->
  <parameter name="mobileNodeType" value="appium" />
  <!-- provide the appium server ip and host for the selenium host and port values -->
  <parameter name="seleniumhost" value="bluefinlvs24.qa.paypal.com" />
  <parameter name="seleniumport" value="4444" />
  <test verbose="2" name="appium-mobile-test" annotations="JDK">
    <classes>
      <class name="andriodTest.AppiumAndroidNativeAppMobileDemo" />
    </classes>
  </test>
</suite>